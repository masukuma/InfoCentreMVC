<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html lang="en-US" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    @(Html.Telerik().StyleSheetRegistrar()
                      .DefaultGroup(group => group
                          .UseTelerikContentDeliveryNetwork(false)
                                  .Add("telerik.examples.min.css")
                                  .Add("telerik.common.min.css")
                          .Add("telerik.windows7.min.css")
                          .Add("telerik.rtl.min.css")
                          .Combined(true)
                          .Compress(true))
                     )
    @RenderSection("HeadContent", required: false)
</head>
<body>
    <div id="page">
        @Html.Partial("_Header")
        <div id="main">
            @RenderBody()
        </div>
        @Html.Partial("_LeftNavigation")
        @Html.Partial("_Footer")
    </div>
    @(Html.Telerik().ScriptRegistrar().DefaultGroup(group => group.Combined(true).Compress(true)))</body>
</html>
